<div class="container">
	<div class="panel panel-default">
    <a href="/admin/class/create" class="btn">Add Class</a>
    <div class="panel-heading">
      <h3>Class</h3>
    </div>

    <div class="panel-body">
      <table class="table">
        <thead>
          <tr>
            <th>Batch</th>
            <th>Section</th>
            <th>Adviser</th>
            <th></th>
          </tr>
        </thead>
        <tbody>
          {{#each classList}}
            <tr>
              <td>{{this.batch}}</td>
              <td>{{this.section}}</td>
              <td>{{this.prefix}} {{this.first_name}} {{this.middle_name}} {{this.last_name}} {{this.suffix}}</td>
              <td><a href="#">view</a></td>
            </tr>
          {{/each}}
        </tbody>
      </table>
    </div>

  </div>
</div>