<div class="container">
	<div class="panel panel-default">
    
    <div class="panel-heading">
      <h3>Class</h3>
    </div>

    <div class="panel-body">
      <table class="table">
        <thead>
          <tr>
            <th>Batch</th>
            <th>Section</th>
            <th></th>
          </tr>
        </thead>
        <tbody>
          {{#each classData}}
            <tr>
              <td>{{this.batch}}</td>
              <td>{{this.section}}</td>
              <td>
                <form action="/faculty/class" method="post">
                  <input type="hidden" value="{{this.id}}" name="classId">
                  <input type="submit" class="btn" value="view" name="view">
                </form></td>
            </tr>
          {{/each}}
        </tbody>
      </table>
    </div>

  </div>
</div>

